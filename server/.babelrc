{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    ["module-resolver", {
      "alias": {
        "config": "../server/src/config",
        "actions": "../server/src/actions",
        "db": "../server/src/db",
        "components": "../server/src/components",
        "core": "../server/src/core"
      }
    }],
    "transform-runtime",
    "transform-decorators-legacy",
    "rewire"
  ]
}

