{
  "name": "Phoenix",
  "description": "BT Mobile Presales website built on a fork from React Starter Kit. ",
  "homepage": "https://www.productsandservices.bt.com/mobile/index.html",
  "scripts": {
    "docs": "esdoc -c esdoc.json",
    "start": "nodemon --watch ../server --ignore ../server/public --delay 1500ms --exec babel-node tools/run start",
    "start:production": "nodemon --watch ../server --ignore ../server/public --delay 1500ms --exec babel-node tools/run start --production",
    "test": "nyc --stack-size=255000  mocha-webpack --require test/utils/dom.js --webpack-config tools/webpack-configs/webpack.mocha.babel.js \"src/**/*.spec.jsx\" --reporter mochawesome --reporter-options reportDir=reports reportFilename=index",
    "test:watch": "node --stack-size=255000 --max_old_space_size=8096 ./node_modules/mocha-webpack/bin/mocha-webpack --require test/utils/dom.js --webpack-config tools/webpack-configs/webpack.mocha.babel.js \"src/**/*.spec.jsx\" --watch --reporter mochawesome --reporter-options reportDir=reports reportFilename=index",
    "test:debug": "node --inspect ./node_modules/mocha-webpack/bin/mocha-webpack --require test/utils/dom.js --webpack-config tools/webpack-configs/webpack.mocha.babel.js \"src/**/*.spec.jsx\" --watch --reporter mochawesome --reporter-options reportDir=reports reportFilename=index",
    "db:export": "mongodump --out ../dump/ --db api",
    "db:import": "mongorestore ../dump/",
    "test:db:export": "npm run test:db:export:documents && npm run test:db:export:documentsHistory && npm run test:db:export:documentRows && npm run test:db:export:documentRowsHistory && npm run test:db:export:collections && npm run test:db:export:collectionsHistory && npm run test:db:export:collectionRows && npm run test:db:export:collectionRowsHistory",
    "test:db:export:documents": "mongoexport -d api -c documents --pretty --jsonArray --out ./test/mock-data/documents/default.json",
    "test:db:export:documentsHistory": "mongoexport -d api -c documentsHistory --pretty --jsonArray --out ./test/mock-data/documentsHistory/default.json",
    "test:db:export:documentRows": "mongoexport -d api -c documentRows --pretty --jsonArray --out ./test/mock-data/documentRows/default.json",
    "test:db:export:documentRowsHistory": "mongoexport -d api -c documentRowsHistory --pretty --jsonArray --out ./test/mock-data/documentRowsHistory/default.json",
    "test:db:export:collections": "mongoexport -d api -c collections --pretty --jsonArray --out ./test/mock-data/collections/default.json",
    "test:db:export:collectionsHistory": "mongoexport -d api -c collectionsHistory --pretty --jsonArray --out ./test/mock-data/collectionsHistory/default.json",
    "test:db:export:collectionRows": "mongoexport -d api -c collectionRows --pretty --jsonArray --out ./test/mock-data/collectionRows/default.json",
    "test:db:export:collectionRowsHistory": "mongoexport -d api -c collectionRowsHistory --pretty --jsonArray --out ./test/mock-data/collectionRowsHistory/default.json"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "dist": "./coverage",
    "fixWebpackSourcePaths": true,
    "reporter": [
      "html"
    ],
    "exclude": [
      ".tmp",
      "test"
    ]
  },
